<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel>
		<g:FormPanel ui:field="form" styleName="uploadFormPanel">
			<g:HorizontalPanel ui:field="filePanel" styleName="popupFieldContainer" spacing="1" verticalAlignment="middle">
				<g:InlineLabel ui:field="file" text="Label file:"></g:InlineLabel>
				<g:FileUpload ui:field="upload" name="upload"></g:FileUpload>
			</g:HorizontalPanel>
		</g:FormPanel>
		<g:Label styleName="localSchemaLabel" text="Local schema files" />
		<g:FlowPanel ui:field="localSchemas" styleName="localSchemas" />
		<g:FormPanel ui:field="schemaUploadForm" >
		  <g:HTMLPanel>
            <g:InlineLabel ui:field="schemaFile" text="Add local schema:"></g:InlineLabel>
            <g:FileUpload ui:field="schemaUpload" name="upload"></g:FileUpload>
            <g:SubmitButton text="Add" />
		  </g:HTMLPanel>
		</g:FormPanel>
		<g:FlowPanel styleName="popupButtonContainer">
			<g:Button ui:field="importButton" text="Import"></g:Button>
			<g:Button ui:field="cancelButton" text="Cancel"></g:Button>
		</g:FlowPanel>
	</g:HTMLPanel>
	
</ui:UiBinder> 