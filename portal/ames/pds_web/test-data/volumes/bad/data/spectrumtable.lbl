PDS_VERSION_ID                     = PDS3                                     
                                                                              
RECORD_TYPE                        = "FIXED_LENGTH"                           
RECORD_BYTES                       = 89                                       
FILE_RECORDS                       = 75                                       
                                                                              
^TABLE                             = "spectrumtable.tab"                      
                                                                              
DATA_SET_ID                        = "EAR-A-EMMI-3-FORNASIER-V1.0"            
PRODUCT_NAME                       = "OBSERVING CIRCUMSTANCES"                
PRODUCT_ID                         = "SPECTRUMTABLE_TAB"                      
INSTRUMENT_HOST_ID                 = "OBS056T9"                               
INSTRUMENT_HOST_NAME               = "EUROPEAN SOUTHERN OBSERVATORY NEW       
        TECHNOLOGY TELESCOPE (NTT)"                                           
INSTRUMENT_ID                      = "EMMI"                                   
INSTRUMENT_NAME                    = "ESO MULTIMODE INSTRUMENT"               
TARGET_NAME                        = "ASTEROID"                               
TARGET_TYPE                        = "ASTEROID"                               
START_TIME                         = 2002-11-09                               
STOP_TIME                          = 2005-01-19                               
PRODUCT_CREATION_TIME              = 2008-07-11  /* File uploaded to OLAF */  
REFERENCE_KEY_ID                   = {"FORNASIERETAL2004",                    
        "FORNASIERETAL2007"}                                                  
                                                                              
OBJECT                             = TABLE                                    
 ROWS                              = 75                                       
 ROW_BYTES                         = 89                                       
 INTERCHANGE_FORMAT                = "ASCII"                                  
 COLUMNS                           = 12                                       
 DESCRIPTION                       = "Observational circumstances for the     
        investigated asteroids, taken from Tables 1 and 2 of Fornasier et al. 
        (2007) and Table 2 of Fornasier et al. (2004)."                       
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 1                                        
  NAME                             = "AST_NO"                                 
  DESCRIPTION                      = "Asteroid number"                        
  DATA_TYPE                        = "ASCII_INTEGER"                          
  START_BYTE                       = 1                                        
  BYTES                            = 6                                        
  FORMAT                           = "I6"                                     
  MISSING_CONSTANT                 = 0                                        
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 2                                        
  NAME                             = "PROV_DESIG"                             
  DESCRIPTION                      = "Provisional designation"                
  DATA_TYPE                        = "CHARACTER"                              
  START_BYTE                       = 8                                        
  BYTES                            = 10                                       
  FORMAT                           = "A10"                                    
  MISSING_CONSTANT                 = "-"                                      
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 3                                        
  NAME                             = "FAMILY_NAME"                            
  DESCRIPTION                      = "The name of the family of which the     
        asteroid is a member"                                                 
  DATA_TYPE                        = "CHARACTER"                              
  START_BYTE                       = 19                                       
  BYTES                            = 9                                        
  FORMAT                           = "A9"                                     
  MISSING_CONSTANT                 = "-"                                      
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 4                                        
  NAME                             = "OBS_TIME"                               
  DESCRIPTION                      = "UT time and date of observation"        
  DATA_TYPE                        = "TIME"                                   
  START_BYTE                       = 29                                       
  BYTES                            = 16                                       
  FORMAT                           = "A16"                                    
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 5                                        
  NAME                             = "EXP_TIME"                               
  DESCRIPTION                      = "Total exposure time"                    
  UNIT                             = "SECOND"                                 
  DATA_TYPE                        = "ASCII_INTEGER"                          
  START_BYTE                       = 46                                       
  BYTES                            = 4                                        
  FORMAT                           = "I4"                                     
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 6                                        
  NAME                             = "EXP_COUNT"                              
  DESCRIPTION                      = "The number of exposures combined to     
        produce the spectrum"                                                 
  DATA_TYPE                        = "ASCII_INTEGER"                          
  START_BYTE                       = 51                                       
  BYTES                            = 1                                        
  FORMAT                           = "I1"                                     
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 7                                        
  NAME                             = "TIME_PER_EXP"                           
  DESCRIPTION                      = "The duration of each individual exposure
        combined to produce the spectrum"                                     
  UNIT                             = "SECOND"                                 
  DATA_TYPE                        = "ASCII_INTEGER"                          
  START_BYTE                       = 53                                       
  BYTES                            = 4                                        
  FORMAT                           = "I4"                                     
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 8                                        
  NAME                             = "ASTEROID_AIRMASS"                       
  DESCRIPTION                      = "The airmass of the asteroid at the time 
        of observation"                                                       
  DATA_TYPE                        = "ASCII_REAL"                             
  START_BYTE                       = 58                                       
  BYTES                            = 4                                        
  FORMAT                           = "F4.2"                                   
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 9                                        
  NAME                             = "SOLAR_STAR"                             
  DESCRIPTION                      = "Name of the solar analog star used to   
        reduce the spectrum"                                                  
  DATA_TYPE                        = "CHARACTER"                              
  START_BYTE                       = 63                                       
  BYTES                            = 10                                       
  FORMAT                           = "A10"                                    
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 10                                       
  NAME                             = "STAR_AIRMASS"                           
  DESCRIPTION                      = "The airmass of the star at the time of  
        its observation"                                                      
  DATA_TYPE                        = "ASCII_REAL"                             
  START_BYTE                       = 74                                       
  BYTES                            = 4                                        
  FORMAT                           = "F4.2"                                   
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 11                                       
  NAME                             = "PHASE"                                  
  DESCRIPTION                      = "The phase angle of the asteroid at the  
        time of observation"                                                  
  UNIT                             = "DEGREE"                                 
  DATA_TYPE                        = "ASCII_REAL"                             
  START_BYTE                       = 79                                       
  BYTES                            = 3                                        
  FORMAT                           = "F3.1"                                   
 END_OBJECT                        = COLUMN                                   
                                                                              
 OBJECT                            = COLUMN                                   
  COLUMN_NUMBER                    = 12                                       
  NAME                             = "V_MAG"                                  
  DESCRIPTION                      = "The approximate V magnitude of the      
        asteroid at the time of observation.  V magnitudes for asteroids in   
        the Eurybates family are taken from the Minor Planet  Center, all     
        others are measured by the project."                                  
  UNIT                             = "MAGNITUDE"                              
  DATA_TYPE                        = "ASCII_REAL"                             
  START_BYTE                       = 83                                       
  BYTES                            = 4                                        
  FORMAT                           = "F4.1"                                   
 END_OBJECT                        = COLUMN                                   
                                                                              
END_OBJECT                         = TABLE                                    
END                                                                           
