PDS_VERSION_ID                  = PDS3
LABEL_REVISION_NOTE             = "D.M. OLSON, 2007-08-06"
RECORD_TYPE                     = FIXED_LENGTH
FILE_RECORDS                    = 54
RECORD_BYTES                    = 218
^INDEX_TABLE                    = ("offsetBytes.TAB", 218<BYTES>)   

VOLUME_ID                       = "ASTROM_0101"
INSTRUMENT_HOST_NAME            = "HUBBLE SPACE TELESCOPE"
INSTRUMENT_HOST_ID              = HST
INSTRUMENT_NAME                 = "WIDE FIELD PLANETARY CAMERA 2"
INSTRUMENT_ID                   = WFPC2
PRODUCT_CREATION_TIME           = 2007-08-06T02:16:00

OBJECT                          = INDEX_TABLE
  INTERCHANGE_FORMAT            = ASCII
  ROWS                          = 54
  COLUMNS                       = 8
  ROW_BYTES                     = 218
  INDEX_TYPE                    = SINGLE
  DESCRIPTION                   = "This is a table which contains one
  record per data file."

  OBJECT                        = COLUMN
    NAME                        = FILE_SPECIFICATION_NAME
    DATA_TYPE                   = CHARACTER
    START_BYTE                  = 2
    BYTES                       = 45
    COLUMN_NUMBER               = 1
    FORMAT                      = "A45"
    DESCRIPTION                 = "Name and directory path of the PDS
    label file describing the particular file."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    NAME                        = PRODUCT_ID
    DATA_TYPE                   = CHARACTER
    START_BYTE                  = 50
    BYTES                       = 31
    COLUMN_NUMBER               = 2
    FORMAT                      = "A31"
    DESCRIPTION                 = "Product identifier. This is a
    unique identifier for every data product in the data set. It is
    equivalent to the name of the data file."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    NAME                        = PRODUCT_CREATION_TIME
    DATA_TYPE                   = DATE
    START_BYTE                  = 84
    BYTES                       = 19
    COLUMN_NUMBER               = 3
    FORMAT                      = "A19"
    DESCRIPTION                 = "UTC time that the product was
    created, given in PDS date/time format ('yyyy-mm-ddThh:mm:ss')."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    NAME                        = RING_OBSERVATION_ID
    DATA_TYPE                   = CHARACTER
    START_BYTE                  = 106
    BYTES                       = 45
    COLUMN_NUMBER               = 4
    FORMAT                      = "A45"
    DESCRIPTION                 = "Identifier for the data set, e.g. 
    S/ASTROM/HST/WFPC2/1998-01-01/DIONE - the fields, separated by /, 
    are Saturn, Astrometry, telescope, instrument, date of last 
    observation in the data set, target of the observations."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    NAME                        = TARGET_NAME
    DATA_TYPE                   = CHARACTER
    START_BYTE                  = 154
    BYTES                       = 10
    COLUMN_NUMBER               = 5
    FORMAT                      = "A10"
    DESCRIPTION                 = "Name of the target body."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    NAME                        = START_TIME
    DATA_TYPE                   = DATE
    START_BYTE                  = 167
    BYTES                       = 21
    COLUMN_NUMBER               = 6
    FORMAT                      = "A21"
    DESCRIPTION                 = "Midpoint time of the first 
    observation in the given data file. Given in PDS date/time format
    ('yyyy-mm-ddThh:mm:ss.sss')."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    NAME                        = STOP_TIME
    DATA_TYPE                   = DATE
    START_BYTE                  = 191
    BYTES                       = 21
    COLUMN_NUMBER               = 7
    FORMAT                      = "A21"
    DESCRIPTION                 = "Midpoint time of the last 
    observation in the given data file. Given in PDS date/time format
    ('yyyy-mm-ddThh:mm:ss.sss')."
  END_OBJECT                    = COLUMN

  OBJECT                        = COLUMN
    NAME                        = FILE_RECORDS
    DATA_TYPE                   = ASCII_INTEGER
    START_BYTE                  = 214
    BYTES                       = 3
    COLUMN_NUMBER               = 8
    FORMAT                      = "I3"
    DESCRIPTION                 = "Number of observations in the given
    data file."
  END_OBJECT                    = COLUMN

END_OBJECT                      = INDEX_TABLE
END
