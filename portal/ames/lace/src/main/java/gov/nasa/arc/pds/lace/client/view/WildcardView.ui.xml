<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    
	    <g:FormPanel ui:field="form" styleName="">
	    	<g:FlowPanel>
				<g:HorizontalPanel ui:field="nsPanel" verticalAlignment="middle" styleName="wildcardPanels">
					<g:InlineLabel ui:field="nsLabel" text="Namespace:"></g:InlineLabel>
					<g:ListBox ui:field="nsDropdown"></g:ListBox>
				</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="elementPanel" verticalAlignment="middle" styleName="wildcardPanels">	
					<g:InlineLabel ui:field="elementLabel" text="Element name:"></g:InlineLabel>
					<g:ListBox ui:field="elementDropdown"></g:ListBox>
				</g:HorizontalPanel>
				<g:FlowPanel styleName="popupButtonContainer">
					<g:Button ui:field="insertButton" text="Insert"></g:Button>
					<g:Button ui:field="cancelButton" text="Cancel"></g:Button>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FormPanel>
	

</ui:UiBinder> 