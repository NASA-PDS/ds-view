<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:FlowPanel ui:field="headerPanel" >
		<g:Label ui:field="returnButton" text="&lt;&lt;" styleName="returnButton" title="Return to label manager" />
		<g:HTMLPanel ui:field="returnLink" styleName="font medium returnLink"><a href="http://pdstools.arc.nasa.gov">PDS Tools</a> &gt;</g:HTMLPanel>
		<g:InlineLabel ui:field="pageTitle" styleName="font extraLarge title borderRadius3px"></g:InlineLabel>
		<g:TextBox ui:field="textBox" styleName="font extraLarge title borderRadius3px" visibleLength="33" visible="false"></g:TextBox>
		<g:HTMLPanel styleName="buttonHolder">
			<g:Button ui:field="createButton" styleName="btn" text="Create New Label" visible="false" />
			<g:Button ui:field="importButton" styleName="btn" text="Import Existing Label" visible="false" />
			<g:Button ui:field="exportButton" styleName="btn" text="Export Label" visible="false" />
			<g:Button ui:field="previewButton" styleName="btn" text="Preview Label" visible="false" />
			<g:Button ui:field="settingsButton" styleName="btn" text="Settings" visible="false" />
			<g:Button ui:field="logoutButton" styleName="btn" text="Log Out" visible="false" />
		</g:HTMLPanel>
		<g:HTML ui:field="logoHolder" styleName="logoHolder"></g:HTML>
		<g:FormPanel ui:field="form">
			<g:FlowPanel>
				<g:Hidden ui:field="fileName" name="fileName" />
				<g:Hidden ui:field="desiredName" name="desiredName" />
			</g:FlowPanel>
		</g:FormPanel>		
			
	</g:FlowPanel>
	
</ui:UiBinder> 