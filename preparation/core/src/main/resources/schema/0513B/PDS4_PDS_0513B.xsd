    </annotation>
    <sequence>
      <element name="axis_name" type="pds:ASCII_Short_String_Collapsed" minOccurs="1" maxOccurs="1"> </element>
      <element name="elements" type="pds:elements" minOccurs="1" maxOccurs="1"> </element>
      <element name="unit" type="pds:ASCII_Short_String_Collapsed" minOccurs="0" maxOccurs="1"> </element>
      <element name="sequence_number" type="pds:sequence_number" minOccurs="1" maxOccurs="1"> </element>
      <element name="Axis_Display" type="pds:Axis_Display" minOccurs="0" maxOccurs="1"> </element>
      <element name="Band_Bin_Set" type="pds:Band_Bin_Set" minOccurs="0" maxOccurs="1"> </element>
    </sequence>
  </complexType>

