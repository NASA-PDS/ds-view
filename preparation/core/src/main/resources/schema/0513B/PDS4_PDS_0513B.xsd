      <element name="comment" type="pds:ASCII_Text_Preserved" minOccurs="0" maxOccurs="1"> </element>
      <element name="Time_Coordinates" type="pds:Time_Coordinates" minOccurs="1" maxOccurs="1"> </element>
      <element name="Primary_Result_Description" type="pds:Primary_Result_Description" minOccurs="0" maxOccurs="1"> </element>
      <element name="Investigation_Area" type="pds:Investigation_Area" minOccurs="1" maxOccurs="unbounded"> </element>
      <element name="Observing_System" type="pds:Observing_System" minOccurs="0" maxOccurs="unbounded"> </element>
      <element name="Target_Identification" type="pds:Target_Identification" minOccurs="1" maxOccurs="unbounded"> </element>
      <element name="Geometry" type="pds:Geometry" minOccurs="0" maxOccurs="unbounded"> </element>
      <element name="Cartography" type="pds:Cartography" minOccurs="0" maxOccurs="1"> </element>
