      <documentation> The mission area allows the insertion of mission specific metadata. </documentation>
    </annotation>
    <sequence>
      <!-- When creating a specific XML schema, remove the 'any' element. You may insert any described nondigital object, one or more times. -->
      <any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      <!-- <element name="Any_NonDigital_Object" type="pds:Any_NonDigital_Object" minOccurs="0" maxOccurs="unbounded"> </element> -->
    </sequence>
  </complexType>

  <complexType name="Node_Area">
