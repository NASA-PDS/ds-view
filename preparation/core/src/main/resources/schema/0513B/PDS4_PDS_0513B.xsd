    </sequence>
  </complexType>

  <complexType name="Time_Coordinates">
    <annotation>
      <documentation> The Time_Coordinates class provides a list of time coordinates. </documentation>
    </annotation>
    <sequence>
      <element name="start_date_time" type="pds:ASCII_Date_Time_YMD" minOccurs="1" maxOccurs="1"> </element>
      <element name="stop_date_time" type="pds:ASCII_Date_Time_YMD" minOccurs="1" maxOccurs="1"> </element>
      <element name="local_mean_solar_time" type="pds:local_mean_solar_time" minOccurs="0" maxOccurs="1"> </element>
      <element name="local_true_solar_time" type="pds:local_true_solar_time" minOccurs="0" maxOccurs="1"> </element>
      <element name="solar_longitude" type="pds:solar_longitude" minOccurs="0" maxOccurs="1"> </element>
    </sequence>
  </complexType>

  <complexType name="Primary_Result_Description">
    <annotation>
      <documentation> The Primary_Result_Description class provides a high-level description of the types of products included in the collection or bundle </documentation>
    </annotation>
    <sequence>
      <element name="type" type="pds:type" minOccurs="0" maxOccurs="1"> </element>
      <element name="purpose" type="pds:purpose" minOccurs="1" maxOccurs="1"> </element>
      <element name="data_regime" type="pds:data_regime" minOccurs="1" maxOccurs="1"> </element>
      <element name="reduction_level" type="pds:reduction_level" minOccurs="1" maxOccurs="1"> </element>
      <element name="description" type="pds:ASCII_Text_Preserved" minOccurs="0" maxOccurs="1"> </element>
    </sequence>
    <assert test="pds:data_regime = ('Dust:Dust_Particle_Counts', 'Dust:Impacts', 'Dust:Mass_Spectra', 'Electric_Field', 'Electrons', 'Far_Infrared', 'Gamma_Ray', 'Infrared', 'Ions', 'Magnetic_Field', 'Microwave', 'Millimeter', 'Near_Infrared', 'Optical', 'Particles', 'Radio', 'Sub-Millimeter', 'Ultraviolet', 'Visible', 'X-Ray')"/>
    <assert test="pds:type = ('Astrometry', 'B-Field_Vectors', 'Column_Density', 'Comet_Nuclear_Properties', 'Count', 'Count_Rate', 'Data_Number', 'Digital_Elevation_Model', 'E-Field_Vectors', 'Environment', 'Environment:Hardware_Temperature', 'Filter_Curve', 'Flux', 'Flux:Electron', 'Flux:Ion', 'Flux:Photometric', 'Gravity_Model', 'Gravity_Model:Spherical_Harmonics', 'Image', 'Image:Anaglyph', 'Image:Color', 'Image:Filtered', 'Impact_Characteristics', 'Instrument', 'Instrument:Geometry', 'Instrument:Parameters', 'Lightcurve', 'Magnetometry', 'Magnitude', 'Map', 'Map:Radiance', 'Map:Thermal', 'Morphology', 'Null_Result', 'Photometry', 'Photometry:Aperture', 'Photometry:High-Speed', 'Photometry:Magnitude', 'Point_Spread_Function', 'Polarimetry', 'Pressure', 'Production_Rate', 'Pulse_Heights', 'Radiometry', 'Shape_Model', 'Shape_Model:Spherical_Harmonics', 'Shape_Model:Triangular_Plate', 'Spectrometry', 'Spectrophotometry', 'Spectroscopy', 'Spectrum', 'Spectrum:Frequency', 'Spectrum:Grism', 'Spectrum:Image', 'Spectrum:Lines', 'Spectrum:Mass', 'Spectrum:Time-Of-Flight', 'Spin_State')"/>
    <assert test="pds:purpose = ('Calibration', 'Checkout', 'Engineering', 'Navigation', 'Science')"/>
    <assert test="pds:reduction_level = ('Analytic', 'Calibrated', 'Derived', 'Raw')"/>
  </complexType>

  <complexType name="Document_Format_Set">
    <annotation>
      <documentation> The Document Format Set class is a set consisting of a document format and associated files. </documentation>
    </annotation>
    <sequence>
      <element name="Document_File" type="pds:Document_File" minOccurs="1" maxOccurs="unbounded"> </element>
      <element name="Document_Format" type="pds:Document_Format" minOccurs="1" maxOccurs="1"> </element>
    </sequence>
  </complexType>

  <complexType name="Investigation_Area">
    <annotation>
      <documentation> The Investigation_Area class provides iformation about an investigation (mission, observing campaign or other coordinated, large-scale data collection effort). </documentation>
    </annotation>
    <sequence>
      <element name="name" type="pds:ASCII_Short_String_Collapsed" minOccurs="1" maxOccurs="1"> </element>
      <element name="type" type="pds:ASCII_Short_String_Collapsed" minOccurs="1" maxOccurs="1"> </element>
      <element name="Internal_Reference" type="pds:Internal_Reference" minOccurs="1" maxOccurs="1"> </element>
    </sequence>
    <assert test="pds:Investigation_Area/pds:Internal_Reference/pds:reference_type = ('has_investigation')"/>
  </complexType>

  <complexType name="Observing_System">
    <annotation>
      <documentation> The Observing System class describes the entire suite used to collect the data. </documentation>
    </annotation>
    <sequence>
      <element name="name" type="pds:ASCII_Short_String_Collapsed" minOccurs="0" maxOccurs="1"> </element>
      <element name="description" type="pds:ASCII_Text_Preserved" minOccurs="0" maxOccurs="1"> </element>
      <element name="Observing_System_Component" type="pds:Observing_System_Component" minOccurs="1" maxOccurs="unbounded"> </element>
    </sequence>
  </complexType>

  <complexType name="Observing_System_Component">
    <annotation>
      <documentation> The Observing System Component class references one or more subsystems used to collect data. A subsystem can be an instrument_host, instrument, or any other similar product. Each subsystem is categorized as either a sensor or a source. If the observing system includes both a sensor and a source, Observing System Component occurs twice (once for each type) otherwise it only occurs once. </documentation>
    </annotation>
    <sequence>
      <element name="name" type="pds:ASCII_Short_String_Collapsed" minOccurs="1" maxOccurs="1"> </element>
      <element name="observing_system_component_type" type="pds:observing_system_component_type" minOccurs="1" maxOccurs="1"> </element>
      <element name="description" type="pds:ASCII_Text_Preserved" minOccurs="0" maxOccurs="1"> </element>
      <element name="Internal_Reference" type="pds:Internal_Reference" minOccurs="0" maxOccurs="1"> </element>
      <element name="External_Reference" type="pds:External_Reference" minOccurs="0" maxOccurs="unbounded"> </element>
    </sequence>
    <assert test="pds:observing_system_component_type = ('Analyst', 'Artificial_Illumination', 'Ground-based_Laboratory', 'Ground-based_Observatory', 'Ground-based_Telescope', 'Instrument', 'Literature_Search', 'PDS_Archived_Data', 'Spacecraft')"/>
    <assert test="pds:Observing_System_Component/pds:Internal_Reference/pds:reference_type = ('has_instrument', 'has_instrument_host', 'has_detector')"/>
  </complexType>

  <complexType name="Target_Identification">
    <annotation>
      <documentation> The Target_Identification class provides detailed target identification information. </documentation>
    </annotation>
