  <complexType name="Delivery_Manifest">
    <annotation>
      <documentation> The Delivery_Manifest class defines a two column table for file references. The table structure is compatible with the output from an MD5 checksum utility. </documentation>
    </annotation>
    <complexContent>
      <extension base="pds:Table_Base">
        <sequence>
          <element name="reference_type" type="pds:reference_type" minOccurs="1" maxOccurs="1"> </element>
          <element name="Record_Character" type="pds:Record_Character" minOccurs="1" maxOccurs="1"> </element>
        </sequence>
        <assert test="pds:fields = ('2')"/>
        <assert test="pds:encoding_type = ('CHARACTER')"/>
        <assert test="pds:reference_type = ('has_member')"/>
        <assert test="pds:Record_Character/Field_Character[1]/field_number eq '1'"/>
        <assert test="pds:Record_Character/Field_Character[1]/field_location eq '1'"/>
        <assert test="pds:Record_Character/Field_Character[1]/data_type eq 'ASCII_MD5_Checksum'"/>
        <assert test="pds:Record_Character/Field_Character[1]/name eq 'MD5_Checksum'"/>
        <assert test="pds:Record_Character/Field_Character[2]/field_number eq '2'"/>
        <assert test="pds:Record_Character/Field_Character[2]/field_location  eq '17'"/>
        <assert test="pds:Record_Character/Field_Character[2]/data_type eq 'ASCII_File_Specification_Name'"/>
        <assert test="pds:Record_Character/Field_Character[2]/name eq 'File_Specification_Name'"/>
      </extension>
    </complexContent>
  </complexType>

