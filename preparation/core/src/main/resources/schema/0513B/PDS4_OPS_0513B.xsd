      <element name="Document_File" type="pds:Document_File" minOccurs="1" maxOccurs="unbounded"> </element>
      <element name="Document_Format" type="pds:Document_Format" minOccurs="1" maxOccurs="1"> </element>
    </sequence>
  </complexType>

