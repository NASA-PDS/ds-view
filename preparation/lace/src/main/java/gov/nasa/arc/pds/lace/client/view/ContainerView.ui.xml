<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
				
	<g:HTMLPanel ui:field="container">
		<g:HTMLPanel ui:field="titlePanel" styleName="hide">
			<g:Image ui:field="expandIcon" />			
			<img ui:field="typeIcon" class="middle" />
			<span ui:field="title" class="title font large"></span>
		</g:HTMLPanel>
		<g:FlowPanel ui:field="contentPanel" styleName="editorContentPanel hide content containerContent"></g:FlowPanel>
	</g:HTMLPanel>	
	
</ui:UiBinder> 