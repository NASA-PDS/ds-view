<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
				
	<g:HTMLPanel ui:field="container">
		<g:HTMLPanel ui:field="titlePanel" styleName="hide">
			<g:Image ui:field="arrowIcon" />	
			<div ui:field="typeIcon" class="icon middle">
    			<div class="alertIcon"></div>
  			</div>
			<span ui:field="title" class="font large middle"></span>
		</g:HTMLPanel>
		<g:FlowPanel ui:field="contentPanel" styleName="editorContentPanel hide content containerContent"></g:FlowPanel>
	</g:HTMLPanel>	
	
</ui:UiBinder> 