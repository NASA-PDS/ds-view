<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:FlowPanel ui:field="headerPanel" styleName="font extraLarge">
		<g:InlineLabel ui:field="templateName">Default Template</g:InlineLabel>
		<g:Button ui:field="importButton" styleName="import" text="Import a PDS4 Label"></g:Button>		
		<g:Button ui:field="exportButton" styleName="export" text="Export Label"></g:Button>

		<g:HTMLPanel styleName="logoContainer">			
			<g:Image ui:field="laceLogo" altText="LACE Logo"></g:Image>
			<g:Image ui:field="nasaLogo" altText="NASA Logo"></g:Image>			
		</g:HTMLPanel>
				
		<g:FormPanel ui:field="form">
			<g:FlowPanel>
				<g:Hidden ui:field="fileName" name="fileName" />
				<g:Hidden ui:field="desiredName" name="desiredName" />
			</g:FlowPanel>
		</g:FormPanel>		
			
	</g:FlowPanel>
	
</ui:UiBinder> 