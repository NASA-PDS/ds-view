<cas:elements xmlns:cas="http://oodt.jpl.nasa.gov/1.0/cas">
  <element id="urn:oodt:ProductId" name="CAS.ProductId">
    <dcElement>Identifier</dcElement>
    <description>Internally generated identifier for the product.
    </description>
  </element>
  <element id="urn:oodt:ProductName" name="CAS.ProductName">
    <dcElement>Title</dcElement>
    <description>Name of the product.
    </description>
  </element>
  <element id="urn:oodt:ProductReceivedTime" name="CAS.ProductReceivedTime">
    <dcElement/>
    <description>Date and time the product was ingested represented in local time and formatted as YYYY-MM-DD'T'hh:mm:ss.sss[Z|[+|-]hh:mm].
    </description>
  </element>

  <element id="urn:oodt:FileLocation" name="FileLocation">
    <dcElement/>
    <description>Directory where the file is located in the archive.
    </description>
  </element>
  <element id="urn:oodt:Filename" name="Filename">
    <dcElement/>
    <description>Name of the file.
    </description>
  </element>
  <element id="urn:oodt:MimeType" name="MimeType">
    <dcElement/>
    <description>The IETF mime type of this product.
    </description>
  </element>
  <element id="urn:oodt:ProductStructure" name="ProductStructure">
    <dcElement/>
    <description>Indicates whether or not a product is Flat (e.g., a set of independent files), or hierarchical, e.g., a directory structure.
    </description>
  </element>
  <element id="urn:oodt:ProductType" name="ProductType">
    <dcElement/>
    <description>Type of the product.
    </description>
  </element>

  <element id="urn:pds:ProductClass" name="ProductClass">
    <dcElement/>
    <description>The PDS product class for the associated product.
    </description>
  </element>
</cas:elements>
