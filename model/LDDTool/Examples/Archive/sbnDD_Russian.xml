    <!-- Coordinate System Identifier: This is a standard value list.  -->
    
    <DD_Attribute>
        <name>coordinate_system_id</name>
        <version_id>1.-</version_id>
        <local_identifier>coordinate_system_id</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Raugh</submitter_name>
        <definition>
            One of several fixed values indicating the coordinate system
            epoch and equinox.  The tag "of date" indicates equinox of date, otherwise the 
            equinox is taken to the be the same as the epoch.
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>true</enumeration_flag>
            <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
            <DD_Permissible_Value>
                <value>J2000</value>
                <value_meaning>
                    Epoch J2000.0, equinox J2000.0
                </value_meaning>
            </DD_Permissible_Value>
            <DD_Permissible_Value>
                <value>J2000_of_date</value>
                <value_meaning>
                    Epoch J2000.0, equinox of date
                </value_meaning>
            </DD_Permissible_Value>
            <DD_Permissible_Value>
                <value>B1950</value>
                <value_meaning>
                    Epoch B1950.0, equinox B1950.0
                </value_meaning>
            </DD_Permissible_Value>
            <DD_Permissible_Value>
                <value>B1950_of_date</value>
                <value_meaning>
                    Epoch 1950.0, equinox of date
                </value_meaning>
            </DD_Permissible_Value>
        </DD_Value_Domain>
    </DD_Attribute>
    
