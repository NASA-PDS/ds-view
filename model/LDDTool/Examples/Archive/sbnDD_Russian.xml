    <!-- UTC times -->
    
    <DD_Attribute>
        <name>start_UTC</name>
        <version_id>1.0</version_id>
        <local_identifier>start_UTC</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Raugh</submitter_name>
        <definition>
            Start time of an observation, in UTC.  This will generally be redundant 
            with start_date_time elsewhere in the label, but this attribute does allow
            a leading negative sign, and must end with the 'Z' UTC time indicator.
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>false</enumeration_flag>
            <value_data_type>ASCII_Date_Time_UTC</value_data_type>
        </DD_Value_Domain>
    </DD_Attribute>
    
    <DD_Attribute>
        <name>mid_UTC</name>
        <version_id>1.0</version_id>
        <local_identifier>mid_UTC</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Raugh</submitter_name>
        <definition>
            Midpoint of an observation, in UTC.  This attribute does allow a leading
            negative sign, and requires the 'Z' UTC specifier.
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>false</enumeration_flag>
            <value_data_type>ASCII_Date_Time_UTC</value_data_type>
        </DD_Value_Domain>
    </DD_Attribute>
    
    <DD_Attribute>
        <name>stop_UTC</name>
        <version_id>1.0</version_id>
        <local_identifier>stop_UTC</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Raugh</submitter_name>
        <definition>
            End time of an observation, in UTC.  This attribute will generally be 
            redundant with stop_date_time listed elsewhere in the label, but this
            attribute will allow a leading negative sign, and requires the 'Z' UTC
            indicator.
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>false</enumeration_flag>
            <value_data_type>ASCII_Date_Time_UTC</value_data_type>
        </DD_Value_Domain>
    </DD_Attribute>
    
    <DD_Attribute>
        <name>time_from_closest_approach</name>
        <version_id>1.0</version_id>
        <local_identifier>time_from_closesr_approach</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Raugh</submitter_name>
        <definition>
            This represents a period of time counting down to closest approach to the
            target, represented as a single floating point number of some time unit, 
            such as hours or seconds.  Periods before closest approach are positive; 
            times after closest approach are negative.
            
            You must specify the time unit for this attribute.
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>false</enumeration_flag>
            <value_data_type>ASCII_Real</value_data_type>
            <unit_of_measure_type>Units_of_Time</unit_of_measure_type>
        </DD_Value_Domain>
    </DD_Attribute>
    
    <DD_Attribute>
        <name>target_time_from_perihelion</name>
        <version_id>1.0</version_id>
        <local_identifier>target_time_from_perihelion</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Raugh</submitter_name>
        <definition>
            This attribute represents the amount of time between the recorded mid-point 
            (or start time, if no mid-point time is indicated) of the observation and 
            perihelion passage of the target.  It is stated as a real number of some
            time unit like days or hours.  Times after aphelion and before perihelion
            are positive; times after perihelion and before aphelion are negative.
            
            You must indicate the unit of measure when using this attribute.
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>false</enumeration_flag>
            <value_data_type>ASCII_Real</value_data_type>
            <unit_of_measure_type>Units_of_Time</unit_of_measure_type>
        </DD_Value_Domain>
    </DD_Attribute>
    
    <DD_Attribute>
        <name>exposure_time</name>
        <version_id>1.0</version_id>
        <local_identifier>exposure_time</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Raugh</submitter_name>
        <definition>
            The amount of time over which the data were taken for this observation.
            This is expressed as a real number of some time unit - minutes or milliseconds,
            for example.  You must indicate the unit of measure for this attribute.
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>false</enumeration_flag>
            <value_data_type>ASCII_Real</value_data_type>
            <minimum_value>0.0</minimum_value>
            <unit_of_measure_type>Units_of_Time</unit_of_measure_type>
            <specified_unit_id>sec</specified_unit_id>
        </DD_Value_Domain>
    </DD_Attribute>
    
    
