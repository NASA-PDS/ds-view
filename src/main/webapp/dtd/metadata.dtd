<!-- metadata (root element): nothing more than a series of key-value pairs -->
<!ELEMENT metadata (keyval)*>

<!-- keyval: a single key/value pair -->
<!ELEMENT keyval (key, val)>

<!-- key: the key part of a key/value pair -->
<!ELEMENT key (#PCDATA)>

<!-- val: the value part of a key/value pair -->
<!ELEMENT val (#PCDATA)>
